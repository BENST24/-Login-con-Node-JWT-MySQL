<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h2>Mis Instrumentales</h2>
    <div class="row">
      <% instrumentals.forEach(function(instrumental) { %>
        <div class="col-md-4">
          <div class="card">
            <h5 class="card-title"><%= instrumental.title %></h5>
            <p class="card-text"><%= instrumental.description %></p>
            <a href="/instrumentals/<%= instrumental.id %>/edit" class="btn">Modificar</a>
            <a href="/instrumentals/<%= instrumental.id %>/delete" class="btn btn-danger">Eliminar</a>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
</div>
</body>
</html>